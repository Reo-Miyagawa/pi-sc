<div class="index">
  <div class="toppage">
    <div class="listwrapper">
      <% @lists.each do |list| %>
      <div class="list">
        <div class="list_header"><%= list.title %></div>
        <div slass="list_header_action">
          <%= link_to edit_list_path(list.id), class: "edpen" do %>
            <i class="fas fa-edit"></i>
          <% end %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>